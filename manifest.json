{
  "manifest_version": 2,
  "name": "快递查询demo",
  "version": "0.0.1",
  "description": "快递单号查询 支持申通，圆通，韵达，中通，EMS等多家快递公司单号查询",
  "background": {
    "scripts": []
  },
  "browser_action": {
    "default_title": "快递单号查询 支持申通，圆通，韵达，中通，EMS等多家快递公司单号查询",
    "default_popup": "popup.html"
  },
  "content_scripts": [

  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "omnibox": { "keyword" : "kd" }
}
